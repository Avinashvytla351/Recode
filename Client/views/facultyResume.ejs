    <style>
      body {
        background: rgb(248, 248, 250);
        font-family: poppins;
        margin: 0;
        padding: 0;
      }
      html {
        scroll-behavior: smooth;
      }
      ::-webkit-scrollbar {
        width: 10px;
      }
      ::-webkit-scrollbar-track {
        border-radius: 10px;
        background: transparent;
      }
      ::-webkit-scrollbar-thumb {
        background: rgb(155, 155, 155);
        border-radius: 10px;
      }
      ::-webkit-scrollbar-thumb:hover {
        background: rgb(110, 110, 110);
      }
      span {
        display: block;
      }
      .main-container {
        width: 100%;
        height: fit-content;
        display: flex;
        align-items: center;
        justify-content: space-between;
      }
      .nav {
        width: 230px;
        position: relative;
        position: fixed;
        left: 0px;
        top: 0px;
        background-color: #fff;
        border-radius: 0.25rem;
        box-shadow: 0 0 0.125rem 0 rgba(0, 0, 0, 0.15);
        display: block;
      }
      .navigation {
        width: 100%;
        height: 98vh;
        display: flex;
        flex-direction: column;
        padding: 7px 0;
        overflow: hidden;
      }
      .links {
        text-decoration: none;
        color: rgb(0, 0, 0);
        font-weight: 400;
        display: block;
        width: 100%;
        text-align: left;
        padding: 16px 15px;
        border-bottom: 0.25mm rgba(0, 0, 0, 0.1) solid;
        display: flex;
        align-items: center;
      }
      .links span {
        font-size: larger;
        margin-left: 5px;
      }
      .links:hover {
        background: rgb(1, 120, 238);
        color: white;
        box-shadow: 0 0 0.335rem 0 rgba(0, 0, 0, 0.322);
      }
      .links:nth-child(1) {
        background: rgb(79, 79, 79);
        color: white;
      }
      .nonepartrequired {
        flex-basis: 265px;
        width: 265px;
        height: 100vh;
        float: left;
      }
      .form-container {
        width: 100%;
      }
      form {
        margin: auto;
        display: block;
        width: 740px;
      }
      .cover {
        display: block;
        background-color: #fff;
        border-radius: 0.25rem;
        box-shadow: 0 0 0.125rem 0 rgba(0, 0, 0, 0.15);
        padding: 40px;
        width: 700px;
        border-top: rgb(1, 120, 238) solid 5px;
        border-bottom: dashed rgb(79, 79, 79) 2px;
        margin-bottom: 75px;
      }
      .title {
        font-size: 20px;
        font-weight: bold;
        margin-bottom: 10px;
        margin-top: 20px;
      }
      .long {
        width: 680px;
      }
      .inputs {
        width: 100%;
        display: flex;
        align-items: center;
        justify-content: space-between;
      }
      .inputfield {
        width: fit-content;
        height: max-content;
        position: relative;
      }
      .inputf {
        font-size: medium;
        font-weight: 400;
        font-family: poppins;
        color: rgb(0, 0, 0);
        outline: none;
        border: none;
        border: rgb(143, 143, 143) solid 0.25mm;
        padding: 10px;
        margin: 25px 0;
        background: rgb(255, 255, 255);
        border-radius: 1mm;
      }
      .inputf:hover {
        border: rgb(0, 0, 0) solid 0.25mm;
      }
      .inputf:focus {
        border: rgb(25, 129, 234) solid 2px;
      }
      .inputf:focus::placeholder {
        opacity: 0;
      }
      .inputf::placeholder {
        color: rgb(145, 145, 145);
      }
      .label {
        position: absolute;
        top: 25px;
        left: 5px;
        font-size: 12px;
        font-family: poppins;
        color: rgb(1, 120, 238);
        opacity: 0;
        transition: 0.25s;
      }
      .inputf:focus + .label {
        opacity: 1;
        top: 5px;
        left: 0;
      }
      .inputf:not(:placeholder-shown) + .label {
        opacity: 1;
        top: 5px;
        left: 0;
      }
      .small {
        width: 250px;
      }
      .vsmall {
        width: 50px;
      }
      .imagefield {
        width: fit-content;
        height: fit-content;
      }
      .imagefield img {
        width: 140px;
        height: 160px;
        border: 0.25mm rgba(0, 0, 0, 0.167) solid;
      }
      .mlong {
        width: 400px;
      }
      .button-block {
        width: 100%;
        display: flex;
        align-items: center;
        justify-content: space-between;
        margin-top: 30px;
      }
      .button-block button {
        background: rgb(1, 120, 238);
        outline: none;
        border: none;
        padding: 10px 15px;
        border-radius: 1mm;
        color: white;
        display: flex;
        align-items: center;
        justify-content: space-evenly;
        font-size: medium;
        cursor: pointer;
        box-shadow: 1px 1px 0.143rem 0 rgba(0, 0, 0, 0.279);
      }
      .button-block button span {
        font-size: larger;
        margin-left: 3px;
      }
      .button-block button:hover {
        background: rgb(0, 90, 179);
      }
      .button-block button:nth-child(2) {
        background: rgb(79, 79, 79);
      }
      .button-block button:nth-child(2):hover {
        background: rgb(55, 55, 55);
      }
      footer {
        width: 100%;
      }
      .footer-section {
        display: flex;
        align-items: center;
        justify-content: space-between;
        flex-wrap: wrap;
      }
      .bg-dark {
        padding-top: 20px;
        padding-bottom: 20px;
        margin-top: 75px;
        background-color: #383838;
      }
      .footer-section a:hover {
        color: rgb(255, 255, 255);
      }
      .footer-left {
        width: 50%;
        display: flex;
        align-items: center;
        justify-content: space-evenly;
      }
      .footer-right {
        width: 20%;
        align-items: center;
        justify-content: center;
      }
      .footer-left-titles a {
        text-decoration: none;
        font-family: poppins;
        font-size: x-large;
        color: rgb(232, 232, 232);
        font-weight: 600;
      }
      .footer-left-links a {
        text-decoration: none;
        font-family: poppins;
        font-size: 14px;
        color: rgb(232, 232, 232);
      }
      .footer-right p {
        font-family: poppins;
        font-size: 14px;
        color: rgb(232, 232, 232);
        width: 100%;
        text-align: center;
      }
      @media all and (max-width: 1000px) {
        .nav {
          display: none;
        }
        .nonepartrequired {
          width: 0;
          flex-basis: 0;
        }
      }
    </style>
  </head>
