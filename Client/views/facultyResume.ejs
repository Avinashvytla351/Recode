                <script>
                  var a = document.getElementById("imageURL");
                  var i = document.getElementById("profile");

                  function dispimg() {
                    b = a.value;
                    c = b.indexOf("file/d/");
                    c = c + 7;
                    d = b.indexOf("/view");
                    b = b.slice(c, d);
                    s = "http://drive.google.com/uc?export=view&id=" + b;
                    i.src = s;
                  }
                  a.onchange = function () {
                    dispimg();
                  };
                  document.body.onload = function () {
                    dispimg();
                  };
